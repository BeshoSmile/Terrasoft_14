<%c = @pack%>
	
  <div class="field">
     destination : <%= c.destination%>  
  </div>
  <br>

  <div class="field">
     description : <%= c.description%>
  </div>
  <br>

  <div class="field">
     weight : <%= c.weight%>
  </div>
  <br>

  <div class="field">
     origin : <%= c.origin%>
  </div>
  <br>

  <div class="field">
     packageValue : <%= c.packageValue%>
  </div>
  <br>

  <div class="field">
     expiryDate : <%= c.expiryDate%>
  </div>
  <br>

  <div class="field">
     receiverAddress : <%= c.receiverAddress%>
  </div>
  <br>

    <div class="field">
     receiverMobNumber : <%= c.receiverMobNumber%>
  </div>
  <br>
    <div class="field">
     receiverEmail : <%= c.receiverEmail%>
  </div>
  <br>

  <a href='list'> back</a>
<%#end%>